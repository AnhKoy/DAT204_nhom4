  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT TOP 7 company_name, COUNT(*) AS job_count
FROM [dbo].[fact_job_postings] f
JOIN [dbo].[dim_companies] c ON f.company_id = c.company_id
GROUP BY company_name
ORDER BY job_count DESC;</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="company_name">
          <DataField>company_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="job_count">
          <DataField>job_count</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
